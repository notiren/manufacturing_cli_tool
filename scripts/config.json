{
  "SKIP_ROWS": 3,
  "VALIDATORS": {
    "adapter_edac": {
      "label": "Adapter EDAC data",
      "ext": ".csv",
      "parser": "parse_csv",
      "limits": {
        "json": "../limits/edac_limits.json",
        "root": "sequences"
      },
      "fields": {
        "serial": ["SerialNumber"]
      }
    },
    "poe_eeload": {
      "label": "PoE EELoad data",
      "ext": ".xlsx",
      "parser": "parse_xlsx",
      "limits": {
        "json": "../limits/eeload_limits.json",
        "root": "fixtureLimits"
      },
      "fields": {
        "serial": ["条码"]
      }
    },
    "poe_network": {
      "label": "PoE Network data",
      "ext": ".txt",
      "parser": "parse_txt_json_array",
      "limits": {
        "json": "../limits/network_limits.json",
        "root": "networkTasksLimits"
      },
      "fields": {
        "serial": ["SerialNumber"],
        "key": "NetworkChartType",
        "value": "Value",
        "name": "Name"
      }
    }
  }
}
